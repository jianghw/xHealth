<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#50d2c2</item>
        <item name="colorPrimaryDark">#11bfa9</item>
        <item name="colorAccent">#50d2c2</item>
    </style>

    <style name="Full.AppTheme" parent="AppTheme">
        <!-- 全屏. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!--<item name="android:background">@color/welcome_bg_cl</item>-->
    </style>


    <!-- *********************************************Base 字体 各种样式************************************ -->
    <!--正文字体,标题-->
    <style name="BodyText_Title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/textSize_16</item>
        <item name="android:textColor">@color/color_title_back</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>
    <!--正文字体,内容-->
    <style name="BodyText_Body">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textSize">@dimen/textSize_16</item>
        <item name="android:textColor">@color/color_body_back</item>
    </style>

    <!--正文字体,医疗内容显示-->
    <style name="Record_BodyText" parent="BodyText_Body">
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/padding_6</item>
    </style>

    <!--正文字体,医疗标题显示-->
    <style name="Doctor_TitleText" parent="BodyText_Body">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/color_title_back</item>
    </style>
    <!--正文字体,右边显示-->
    <style name="Right_TitleText" parent="BodyText_Body">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLength">16</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <!--正文字体,医疗内容显示-->
    <style name="Doctor_BodyText" parent="Right_TitleText">
        <item name="android:layout_marginRight">@dimen/margin_26</item>
    </style>

    <!--有外框的 EditText-->
    <style name="EditText_Frame">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/padding_6</item>
        <item name="android:layout_marginBottom">@dimen/padding_6</item>
        <item name="android:layout_marginLeft">@dimen/margin_10</item>
        <item name="android:paddingBottom">@dimen/padding_8</item>
        <item name="android:paddingTop">@dimen/padding_8</item>
        <item name="android:paddingLeft">@dimen/padding_4</item>
        <item name="android:textColor">@color/color_body_back</item>
        <item name="android:background">@drawable/bg_edt_login</item>
        <item name="android:textColorHint">@color/color_label_gray</item>
        <item name="android:textSize">@dimen/textSize_16</item>
    </style>

    <!--有外框的 EditText-->
    <style name="EditText_Frame_extra">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/padding_4</item>
        <item name="android:layout_marginBottom">@dimen/padding_6</item>
        <item name="android:layout_marginLeft">@dimen/padding_6</item>
        <item name="android:paddingBottom">@dimen/padding_4</item>
        <!--<item name="android:paddingTop">@dimen/padding_6</item>-->
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color_body_back</item>
        <item name="android:background">@drawable/bg_edt_login</item>
        <item name="android:textColorHint">@color/color_label_gray</item>
        <item name="android:textSize">@dimen/textSize_16</item>
    </style>
    <!--有外框的 TextView-->
    <style name="TextView_Frame" parent="EditText_Frame_extra">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- login EditText theme. -->
    <style name="Login_EditText" parent="EditText_Frame">
        <item name="android:layout_width">match_parent</item>
        <item name="android:drawablePadding">@dimen/padding_6</item>
        <item name="android:paddingLeft">@dimen/padding_8</item>
        <item name="android:singleLine">true</item>
    </style>

    <!--patientinfo中项text-->
    <style name="Tv_Patient_Info" parent="BodyText_Body">
        <item name="android:layout_marginLeft">@dimen/margin_16</item>
    </style>
    <!--文本输入-->
    <style name="EditText_Content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/margin_10</item>
        <item name="android:layout_marginRight">@dimen/margin_10</item>
        <item name="android:layout_marginTop">@dimen/margin_10</item>
        <!--<item name="android:layout_marginBottom">@dimen/margin_16</item>-->
        <item name="android:background">@drawable/bg_white</item>
        <item name="android:gravity">left|top</item>
        <item name="android:minLines">10</item>
        <item name="android:padding">@dimen/margin_10</item>
    </style>

    <!-- *********************************************Base 控件 各种样式************************************ -->

    <!--LinearLayout 长按键-->
    <style name="Bg_Lay_Press">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/input_height_40</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/bg_white_press</item>
        <item name="android:paddingLeft">@dimen/margin_16</item>
        <item name="android:paddingRight">@dimen/margin_16</item>
    </style>
    <!--长按键中的img-->
    <style name="Lay_ImageView">
        <item name="android:layout_width">@dimen/margin_20</item>
        <item name="android:layout_height">@dimen/margin_20</item>
    </style>

    <!--分隔离线-->
    <style name="Line_Match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/tv_press_color</item>
    </style>
    <!--分隔离线,左右有空隙-->
    <style name="Line_Gap" parent="Line_Match">
        <item name="android:layout_marginLeft">@dimen/margin_16</item>
        <item name="android:layout_marginRight">@dimen/margin_16</item>
    </style>


    <!--外LinearLayout horizontal-->
    <style name="Lay_Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/input_height_48</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!--添加图片-->
    <style name="Add_Image">
        <item name="android:src">@mipmap/ic_add_img</item>
        <item name="android:layout_width">@dimen/margin_48</item>
        <item name="android:layout_height">@dimen/margin_48</item>
    </style>
    <!--删除图标-->
    <style name="Img_Delete">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@mipmap/ic_edit_delete</item>
    </style>
    <!--向右箭头-->
    <style name="Image_Arrow_Right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@mipmap/arrow_right</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="tv_main_personinfo">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/personinfo_marginright</item>
        <item name="android:layout_width">wrap_content</item>
