<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#50d2c2</item>
        <item name="colorPrimaryDark">#11bfa9</item>
        <item name="colorAccent">#50d2c2</item>
    </style>

    <style name="Full.AppTheme" parent="AppTheme">
        <!-- 全屏. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!--<item name="android:background">@color/welcome_bg_cl</item>-->
    </style>

    <!-- login EditText theme. -->
    <style name="Login_EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_edt_login</item>
        <item name="android:drawablePadding">@dimen/login_edit_drawablePadding</item>
        <item name="android:paddingBottom">@dimen/login_edit_paddingTop</item>
        <item name="android:textColorHint">@color/login_edit_hint</item>
        <item name="android:paddingTop">@dimen/login_edit_paddingTop</item>
        <item name="android:paddingLeft">@dimen/login_edit_paddingLeft</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Home_RadioButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color_history</item>
        <item name="android:textSize">@dimen/login_tv_textSize</item>
    </style>
    <!--patientinfo中项LinearLayout-->
    <style name="lay_patientinfoiteam">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
    </style>
    <!--patientinfo中项imageview-->
    <style name="iv_patientinfoiteam">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">35dp</item>
    </style>
    <!--patientinfo中项text-->
    <style name="tv_patientinfoiteam">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">@color/app_tv_color</item>
        <item name="android:textSize">@dimen/login_tv_textSize</item>
    </style>
    <!--patientinfo中项线-->
    <style name="lin_patientinfoiteam">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
