<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/lay_title_medicalhistory"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toptitle_height"
            android:background="@color/kaurihealth_header_background">

            <ImageView
                android:id="@+id/iv_back"
                style="@style/toptitleback" />

            <TextView
                style="@style/toptitletv"
                android:text="医疗团队" />

        </RelativeLayout>



        <GridView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/gv_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:numColumns="2">

        </GridView>

    </LinearLayout>
</RelativeLayout>



