<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical">
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/clinical_SR"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <com.youyou.zllibrary.widget.AnimatedExpandableListView
        android:id="@+id/animexpandableview_content_clinicatreatment"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:dividerHeight="0.5dp"
        android:childDivider="#dfdfdd"
        android:background="#f5f5f5"></com.youyou.zllibrary.widget.AnimatedExpandableListView>
    </android.support.v4.widget.SwipeRefreshLayout>
</LinearLayout>