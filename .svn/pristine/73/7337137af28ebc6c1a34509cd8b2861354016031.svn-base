package com.kaurihealth.datalib.request_bean.builder;

import com.kaurihealth.datalib.request_bean.bean.NewFamilyMemberBean;

/**
 * Created by Garnet_Wu on 2016/11/11.
 * 患者信息--> 家庭成员 --> 添加家庭成员 请求bean的Builder
 */
public class NewFamilyMemberBeanBuilder {
    public NewFamilyMemberBean Build(String firstName, String lastName, String gender, String dateOfBirth, String relationship, int patientId) {

        NewFamilyMemberBean newFamilyMemberBean = new NewFamilyMemberBean();
        newFamilyMemberBean.setFirstName(firstName);
        newFamilyMemberBean.setLastName(lastName);
        newFamilyMemberBean.setGender(gender);
        newFamilyMemberBean.setDateOfBirth(dateOfBirth);
        newFamilyMemberBean.setRelationship(relationship);
        newFamilyMemberBean.setPatientId(patientId);
        return newFamilyMemberBean;
    }


}
