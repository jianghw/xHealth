            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="BP："
                android:textColor="#293d47"
                android:textSize="@dimen/textSize_12"/>

            <EditText
                android:id="@+id/edit_bp_f"
                android:layout_width="0dp"
