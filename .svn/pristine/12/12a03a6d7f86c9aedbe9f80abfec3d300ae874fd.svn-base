<?xml version="1.0" encoding="utf-8"?>

<resources>
    <!--圆形头像-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_border_overlay" format="boolean"/>
        <attr name="civ_fill_color" format="color"/>
    </declare-styleable>
    <!--配置间隔属性-->
    <declare-styleable name="NineGridImageView">
        <attr name="imgGap" format="dimension"/>
    </declare-styleable>
    <!--搜索栏-->
    <declare-styleable name="MaterialSearchBar">
        <attr name="searchIconDrawable" format="reference"/>
        <attr name="hint" format="string"/>
        <attr name="speechMode" format="boolean"/>
        <attr name="hintColor" format="color"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="FlowLayoutAttrs">
        <attr name="horizontalSpacing" format="dimension"/>
        <attr name="verticalSpacing" format="dimension"/>
        <attr name="itemSize" format="dimension"/>
        <attr name="itemColor" format="color"/>
        <attr name="backgroundResource" format="reference"/>
        <attr name="textPaddingH" format="dimension"/>
        <attr name="textPaddingV" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CollapsibleLinearLayout">
        <attr name="CollapsibleTitle" format="string"/>
        <attr name="CollapsibleHintOpen" format="string"/>
        <attr name="CollapsibleHintClose" format="string"/>
        <attr name="CollapsibleHeight" format="dimension"/>
        <attr name="CollapsibleDuration" format="integer"/>
        <attr name="CollapsibleTitleColor" format="color"/>
        <attr name="CollapsibleContentColor" format="color"/>
        <attr name="CollapsibleCornerRadius" format="dimension"/>
        <attr name="CollapsibleTitleTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RecentlyRecordLayout">
        <attr name="RecentlyTitle" format="string"/>
        <attr name="RecentlyContent" format="string"/>
        <attr name="RecentlyContentHint" format="string"/>
        <attr name="RecentlyTitleColor" format="color"/>
        <attr name="RecentlyContentColor" format="color"/>
        <attr name="RecentlyCompileMode" format="boolean"/>
    </declare-styleable>
    <!--辅助检查控件-->
    <declare-styleable name="AuxiliaryExaminationLayout">
        <attr name="AuxiliaryTitle" format="string"/>
        <attr name="AuxiliaryContentF" format="string"/>
        <attr name="AuxiliaryBoxF" format="boolean"/>
        <attr name="AuxiliaryContentS" format="string"/>
        <attr name="AuxiliaryBoxS" format="boolean"/>
        <attr name="AuxiliaryCompileMode" format="boolean"/>
    </declare-styleable>
</resources>