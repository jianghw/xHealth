public class RecordBean {   //RecordDisplayDto
    private int recordId;
    private String recordType;
    private boolean isValid;
    private int ownerId;
    private int patientRecordId;
    /**
     * outPatientRecordId : 1
     * presentIllness : sample string 2
     * isPastIllnessReviewed : true
     * isPastIllnessUpToDate : true
     * hrValue : 1.0
     * upperBpValue : 1.0
     * lowerBpValue : 1.0
     * rrValue : 1.0
     * tValue : 1.0
     * physicalExamination : sample string 5
     * isWorkupReviewed : true
     * isCurrentMedicationReviewed : true
     * isCurrentMedicationUpToDate : true
     * isLongTermMonitoringDataReviewed : true
     * workupReviewComment : sample string 10
     * treatmentAndPlan : sample string 11
     * recordId : 12
     */

    private OutpatientRecordBean outpatientRecord;   //OutpatientRecordDetailDisplayDto
    /**
     * remoteConsultationRecordId : 1
     * presentIllness : sample string 2
     * isPatientHealthRecordReviewed : true
     * currentHealthIssues : sample string 4
     * currentTreatment : sample string 5
     * remoteConsultationDoctorComment : sample string 6
     * recordId : 7
     */

    private RemoteConsultationRecordBean remoteConsultationRecord;  // 	RemoteConsultationRecordDetailDisplayDto

    /**
     * onlineConsultationRecordId : 1
     * onlineConsultationPurpose : sample string 2
     * majorIssue : sample string 3
     * onlineConsultationDoctorComment : sample string 4
     * recordId : 5
     * currentTreatment : sample string 6
     * presentIllness : sample string 7
     */

    private OnlineConsultationRecordBean onlineConsultationRecord;  // 	OnlineConsultationRecordDetailDisplayDto

    public int getRecordId() {
        return recordId;
    }

    public void setRecordId(int recordId) {
        this.recordId = recordId;
    }

    public String getRecordType() {
        return recordType;
    }

    public void setRecordType(String recordType) {
        this.recordType = recordType;
    }

    public boolean isIsValid() {
        return isValid;
    }

    public void setIsValid(boolean isValid) {
        this.isValid = isValid;
    }

    public int getOwnerId() {
        return ownerId;
    }

    public void setOwnerId(int ownerId) {
        this.ownerId = ownerId;
    }

    public int getPatientRecordId() {
        return patientRecordId;
    }

    public void setPatientRecordId(int patientRecordId) {
        this.patientRecordId = patientRecordId;
    }

    public OutpatientRecordBean getOutpatientRecord() {
        return outpatientRecord;
    }

    public void setOutpatientRecord(OutpatientRecordBean outpatientRecord) {
        this.outpatientRecord = outpatientRecord;
    }

    public RemoteConsultationRecordBean getRemoteConsultationRecord() {
        return remoteConsultationRecord;
    }

    public void setRemoteConsultationRecord(RemoteConsultationRecordBean remoteConsultationRecord) {
        this.remoteConsultationRecord = remoteConsultationRecord;
    }

    public OnlineConsultationRecordBean getOnlineConsultationRecord() {
        return onlineConsultationRecord;
    }

    public void setOnlineConsultationRecord(OnlineConsultationRecordBean onlineConsultationRecord) {
        this.onlineConsultationRecord = onlineConsultationRecord;
    }

}
