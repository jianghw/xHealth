public class ReferralPatientEvent {
    private final int mType;
    private int doctorId;

    public ReferralPatientEvent(int doctorId, int type){
        this.doctorId = doctorId;
        this.mType = type;
    }

    public int getType() {
        return mType;
    }

    public int getDoctorId() {
        return doctorId;
    }
}
