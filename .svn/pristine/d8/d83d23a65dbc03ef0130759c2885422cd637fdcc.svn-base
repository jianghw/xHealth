package com.kaurihealth.datalib.request_bean.builder;

import com.kaurihealth.datalib.request_bean.bean.NewReferralMessageAlertBean;

import java.util.Date;

/**
 * 备注：
 */
public class NewReferralMessageAlertBeanBuilder {

    public NewReferralMessageAlertBean Build(Date reminderTime, String reminderContent, int doctorPatientShipId, Date creatTime) {
        NewReferralMessageAlertBean messageAlertBean = new NewReferralMessageAlertBean();
        messageAlertBean.setReminderTime(reminderTime);
        messageAlertBean.setReminderContent(reminderContent);
        messageAlertBean.setDoctorPatientShipId(doctorPatientShipId);
        messageAlertBean.setCreatTime(creatTime);
        return messageAlertBean;
    }
}
