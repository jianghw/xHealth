<?xml version="1.0" encoding="utf-8"?>
<com.daimajia.swipe.SwipeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/swipe"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--先写隐藏的那一层布局  -->
    <LinearLayout
        android:layout_width="@dimen/swipe_width_100"
        android:layout_height="@dimen/input_height_48"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_compile"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/color_gold"
            android:textColor="@color/color_white"
            android:text="@string/swipe_tv_compile"/>

        <TextView
            android:id="@+id/tv_delete"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/color_red"
            android:textColor="@color/color_white"
            android:text="@string/swipe_tv_delete"/>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/input_height_48"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/tv_data"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginLeft="@dimen/margin_16"
            android:textColor="@color/color_body_back"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textColor="@color/color_body_back"
            android:gravity="center"
            android:layout_marginRight="@dimen/margin_16"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
</com.daimajia.swipe.SwipeLayout>
