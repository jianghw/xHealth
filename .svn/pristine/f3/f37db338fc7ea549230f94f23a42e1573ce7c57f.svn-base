<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:gravity="center_vertical"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            style="@style/BodyText_Title"
            android:layout_width="0dp"
            android:layout_height="@dimen/input_height_48"
            android:layout_weight="1"
            android:gravity="center|right"
            android:text="体格检查:"/>

        <TextView
            style="@style/BodyText_Body"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:text="HR"/>

        <EditText
            android:id="@+id/et_physical_hr"
            style="@style/EditText_Frame"
            android:layout_width="0dp"
            android:layout_weight="1.5"
            android:background="@drawable/bg_edt_login"
            android:digits="0123456789."
            tools:text="临床诊疗"/>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            style="@style/BodyText_Title"
            android:layout_width="0dp"
            android:layout_height="@dimen/input_height_48"
            android:layout_weight="1"
            android:gravity="center|right"/>

        <TextView
            style="@style/BodyText_Body"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:text="BP"/>

        <EditText
            android:id="@+id/et_physical_bp_1"
            style="@style/EditText_Frame"
            android:layout_width="0dp"
            android:layout_weight="0.6"
            android:background="@drawable/bg_edt_login"
            android:digits="0123456789."
            tools:text="临床诊疗"/>
        <TextView
            style="@style/BodyText_Body"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:text="--"/>
        <EditText
            android:id="@+id/et_physical_bp_2"
            style="@style/EditText_Frame"
            android:layout_width="0dp"
            android:layout_weight="0.6"
            android:background="@drawable/bg_edt_login"
            android:digits="0123456789."
            tools:text="临床诊疗"/>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            style="@style/BodyText_Title"
            android:layout_width="0dp"
            android:layout_height="@dimen/input_height_48"
            android:layout_weight="1"
            android:gravity="center|right"/>

        <TextView
            style="@style/BodyText_Body"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:text="RR"/>

        <EditText
            android:id="@+id/et_physical_rr"
            style="@style/EditText_Frame"
            android:layout_width="0dp"
            android:layout_weight="1.5"
            android:background="@drawable/bg_edt_login"
            android:digits="0123456789."
            tools:text="临床诊疗"/>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            style="@style/BodyText_Title"
            android:layout_width="0dp"
            android:layout_height="@dimen/input_height_48"
            android:layout_weight="1"
            android:gravity="center|right"/>

        <TextView
            style="@style/BodyText_Body"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:text="T"/>

        <EditText
            android:id="@+id/et_physical_t"
            style="@style/EditText_Frame"
            android:layout_width="0dp"
            android:layout_weight="1.5"
            android:background="@drawable/bg_edt_login"
            android:digits="0123456789."
            tools:text="临床诊疗"/>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical">

        <TextView
            style="@style/BodyText_Title"
            android:layout_width="0dp"
            android:layout_height="@dimen/input_height_48"
            android:layout_weight="1"
            android:gravity="center|right"/>

        <EditText
            android:id="@+id/et_physical_note"
            style="@style/EditText_Frame"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:background="@drawable/bg_edt_login"
            android:hint="备注"/>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"/>
    </LinearLayout>
</merge>