<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="@color/color_label_gray"
                android:centerColor="@color/color_label_gray"
                android:centerY="0.75"
                android:endColor="@color/color_label_gray"
                android:angle="270" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="@color/color_enable_green"
                    android:centerColor="@color/color_enable_green"
                    android:centerY="0.75"
                    android:endColor="@color/color_enable_green"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

</layer-list>

